    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "prettier": "^3.0.1",
    "size-limit": "^9.0.0",
    "ts-to-zod": "^3.1.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "zod": "^3.22.2"
